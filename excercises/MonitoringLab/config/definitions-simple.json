    {"name": "q.orders", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {}}
  ],
  "bindings": [
    {"source": "ex.orders", "vhost": "/", "destination": "q.orders", "destination_type": "queue", "routing_key": "orders", "arguments": {}}
  ],
